{
  "name": "dwc-api",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.26.0",
    "@sap/cds": "^7",
    "@sap/cds-hana": "^2",
    "@sap/xsenv": "^5.1.0",
    "@sap/xssec": "^3",
    "cfenv": "^1.2.4",
    "express": "^4",
    "@cap-js/hana": "^1"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7",
    "@sap/eslint-plugin-cds": "^3",
    "eslint": "^9"
  },
  "scripts": {
    "start": "cds-serve",
    "deploy": "cf deploy mta_archives/dwc-api_1.0.0.mtar --retries 1",
    "approuter-local": "cds bind --exec -- npm start --prefix app/router",
    "watch-hybrid": "cds watch --profile hybrid"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": {
          "kind": "xsuaa"
        }
      },
      "db": "hana",
      "Northwind": {
        "kind": "odata",
        "model": "srv/external/Northwind",
        "[backend]": {
          "credentials": {
            "url": "https://services.odata.org/Experimental/OData/OData.svc"
          }
        },
        "[production]": {
          "credentials": {
            "destination": "NorthWind"
          }
        },
        "[hybrid]": {
          "credentials": {
            "destination": "NorthWind"
          }
        }
      },
      "R_GV001_DMF": {
        "kind": "odata",
        "model": "srv/external/R_GV001_DMF",
        "[backend]": {
          "credentials": {
            "url": "https://ibm-q.us10.hcs.cloud.sap/api/v1/dwc/consumption/relational/RPT_RASRV_001/R_GV001_DMF/$metadata"
          }
        },
        "[hybrid]": {
          "credentials": {
            "destination": "DATASPHERE-DEV-API"
          }
        }
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}
